{
  "name": "gemini",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Provides definitions in the side panel.",
  "side_panel": {
    "default_path": "index.html"
  },
 "content_security_policy": {
  "script-src": "'self' https://esm.run/"
  },
  "permissions": ["sidePanel"],
  "action": {
      "default_title": "click to open"
  }
}